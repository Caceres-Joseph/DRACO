			set_local $calc // LO COLOCO DE NUEVO EN LA POSICION YA CALCULADA..
			// FIN AUMENTO....n
			// FIN ACTUALIZACION
			BR $FOR_0_1_CONDICION // SALTO A LA CONDICION...
	$FOR_0_1_FALSO // SALIDA WHILE

	$e_retornar

